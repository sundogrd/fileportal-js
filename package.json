    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "regenerator-runtime": "^0.12.1",
    "standard-version": "^4.4.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
