    "superagent": "^3.8.3",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.2",
    "@types/sinon": "^5.0.1",
    "babel-core": "^7.0.0-bridge.0",
